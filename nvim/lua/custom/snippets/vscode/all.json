{
	"comment_block": {
		"prefix": "cb",
		"body": [
			"// //////////////////////////////////////////////////",
			"// ${1:CommentBlock}",
			"// //////////////////////////////////////////////////"
		]
	},
	"comment_region": {
		"prefix": "crg",
		"body": [
			"// //////////////////////////////////////////////////",
			"// #region ${1:CommentBlock} {{{",
			"// //////////////////////////////////////////////////",
			"$2",
			"// //////////////////////////////////////////////////",
			"// #endregion ${1:CommentBlock} }}}",
			"// //////////////////////////////////////////////////"
		]
	}
}
